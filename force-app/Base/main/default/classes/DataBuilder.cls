	public static void updateRecords() {
		for (List<Course_Attendee__c> atts : [SELECT ID FROM Course_Attendee__c]) {
			for (Course_Attendee__c att : atts) {
				att.Status__c = 'Enrolled';
			}
			update atts;
		}
	}

	// DataBuilder2.createMoreRecords();
	public static void createMoreRecords() {
		List<Contact> students = [SELECT Id FROM Contact];
		List<Course_Delivery__c> deliveries = [SELECT Id FROM Course_Delivery__c];
		List<Course_Attendee__c> newRecords = new List<Course_Attendee__c>();

		for (Integer i = 0; i < 100; i++) {
			Contact student = students[Math.mod(i, (students.size()-1))];
			Course_Delivery__c delivery = deliveries[Math.mod(i, (deliveries.size()-1))];

			Course_Attendee__c ca = new Course_Attendee__c();
			ca.Course_Delivery__c = delivery.Id;
			ca.Student__c = student.Id;
			ca.Status__c = 'Enrolled';
			newRecords.add(ca);
		}
		insert newRecords;
	}
